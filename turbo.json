{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "clean": {
            "cache": false,
            "outputs": []
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "eslint:check": { "outputs": [] },
        "eslint:fix": { "outputs": [] },
        "prettier:check": { "outputs": [] },
        "prettier:fix": { "outputs": [] },
        "lint": { "outputs": [] },
        "lint:fix": { "outputs": [] },
        "test": {
            "dependsOn": ["^build"],
            "outputs": []
        }
    }
}
